{
    "version": 2,
    "regions": [
        "fra1"
        ],
        "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
        },
        "routes": [
            {
            "src": "/build/(.*)",
            "dest": "/build/$1"
            },
            {
            "src": "/css/(.*)",
            "dest": "/css/$1"
            },
            {
            "src": "/js/(.*)",
            "dest": "/js/$1"
            },
            {
            "src": "/images/(.*)",
            "dest": "/images/$1"
            },
            {
            "src": "/(.*)",
            "dest": "/api/index.php"
            }
        ],
        "outputDirectory": "public"
} 